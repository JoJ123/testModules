{
  "name": "demomodules",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/main.ts",
    "debug": "nodemon --exec ts-node -r tsconfig-paths/register --inspect --debug-brk src/main.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@graphql-modules/core": "0.7.14-alpha-b140687.45+b140687",
    "@graphql-modules/di": "0.7.14-alpha-b140687.45+b140687",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "14.5.8",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "graphql-tag": "^2.10.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  }
}